{
   "env": {
      "es6": true,
      "node": true
   },
   "extends": "eslint:recommended",
   "parser": "babel-eslint",
   "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
   },
   "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
   },
   "rules": {
      "no-console": 0,
      "no-empty": 0,
      "no-irregular-whitespace": 0,
   }
}